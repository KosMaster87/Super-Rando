{
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(node_modules|\\.test\\.js$|\\.html$)"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "template": "./node_modules/docdash"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "docdash": {
      "static": true,
      "sort": true,
      "collapse": true,
      "search": true,
      "includeDate": false,
      "sectionOrder": [
        "Tutorials",
        "Classes",
        "Modules",
        "Externals",
        "Events",
        "Namespaces",
        "Mixins",
        "Interfaces"
      ],
      "color": {
        "primary": "#32291d",
        "secondary": "#514434",
        "background": "#32291d",
        "link": "#bd854c",
        "heading": "#baab7e",
        "text": "#e8dcc0"
      }
    },
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
